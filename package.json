{
  "name": "versions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "ssr:build": "vue-cli-service ssr:build",
    "ssr:fix": "vue-cli-service ssr:fix",
    "ssr:fix-vuex": "vue-cli-service ssr:fix-vuex",
    "ssr:serve": "vue-cli-service ssr:serve --unhandled-rejections=strict",
    "ssr:start": "vue-cli-service ssr:start",
    "ssr:static": "vue-cli-service ssr:static",
    "lintjs": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "tailwind": "postcss css/tailwind.css -o public/statics/assets/css/tailwind.css"
  },
  "dependencies": {
    "@uvue/core": "^0.1.7",
    "@uvue/server": "^0.1.7",
    "autoprefixer": "^10.0.4",
    "axios": "^0.21.0",
    "compression": "^1.7.4",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "json-stringify-safe": "^5.0.1",
    "moment": "^2.29.1",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "serve-static": "^1.14.1",
    "tailwindcss": "^2.0.1",
    "vue": "^2.6.12",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@uvue/vue-cli-plugin-ssr": "^0.1.7",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "vue-template-compiler": "^2.6.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint"
    ]
  }
}
