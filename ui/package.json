{
  "name": "versions",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "server": "vue-tsc --noEmit && vite build && vite preview",
    "lint": "eslint --fix --ext .js,.vue,.ts ./"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.6",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.5.1",
    "chart.js": "^4.4.0",
    "moment": "^2.29.4",
    "vue": "^3.3.6",
    "vue-chart-3": "^3.1.8",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/types": "^7.23.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@types/node": "^20.8.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/compiler-sfc": "^3.3.6",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.20"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix --ext .js,.vue,.ts src",
      "npm run build",
      "git add"
    ]
  }
}