{
  "name": "versions",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.vue,.ts ./"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.24.0",
    "chart.js": "^3.7.0",
    "moment": "^2.29.1",
    "vue": "^3.2.29",
    "vue-chart-3": "^3.0.9",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "lint-staged": "^12.3.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.16",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vue-tsc": "^0.30.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix --ext .js,.vue,.ts src",
      "npm run build",
      "git add"
    ]
  }
}
